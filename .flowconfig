[ignore]
.*/node_modules/flow-bin/.*
.*/node_modules/react-universal-component/.*
.*/node_modules/serviceworker-webpack-plugin/.*
.*/node_modules/webpack-flush-chunks/.*
.*/node_modules/stylelint/.*
.*/build/.*

[include]
./flow-typed/

[libs]

[lints]

[options]
module.name_mapper='.*\(.s?css\)' -> 'empty/object'
